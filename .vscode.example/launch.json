    "version": "0.2.0",
    "configurations": [
        {
            "name": "Analytics",
            "type": "python",
            "request": "launch",
            "module": "dagster",
            "args": [
                "dev",
                "-m",
                "analytics"
            ],
            "justMyCode": true,
            "envFile": ".env"
        },
        {
            "name": "Ingestor",
            "type": "python",
            "request": "launch",
            "module": "ingestor.main",
            "justMyCode": true,
            "envFile": ".env"
        },
        {
            "name": "Plugin",
            "type": "java",
            "request": "launch",
            "mainClass": "com.tracker.TrackerPluginTest",
            "projectName": "tracker"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
